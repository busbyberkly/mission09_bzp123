@{
}
@model List<Book>
@foreach (Book b in Model)
{
    <h3>@b.Title</h3>
    
    <ul>
        <li>@b.Author</li>
        <li>@b.Publisher</li>
        <li>@b.Isbn</li>
        <li>@b.Classification</li>
        <li>@b.Category</li>
        <li>@b.PageCount</li>
        <li>@b.Price</li>
    </ul>
    
    <br />


}